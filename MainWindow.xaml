<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfSimpleCal"                
        xmlns:Properties="clr-namespace:WpfSimpleCal.Properties"
        x:Class="WpfSimpleCal.MainWindow"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance Type=local:MainView}"
        Title="MainWindow" Height="700" Width="1150" ResizeMode="NoResize">
    <Grid>
        <local:MainView HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
    </Grid>
</Window>
